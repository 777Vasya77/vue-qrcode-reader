{
  "name": "vue-qrcode-reader",
  "description": "A set of Vue.js components for detecting and decoding QR codes.",
  "version": "0.0.0-development",
  "source": "src/index.js",
  "main": "dist/vue-qrcode-reader.js",
  "module": "dist/vue-qrcode-reader.mjs",
  "unpkg": "dist/vue-qrcode-reader.umd.js",
  "scripts": {
    "build": "microbundle --external vue --name VueQrcodeReader",
    "watch": "microbundle watch",
    "serve-demo": "static ./dist"
  },
  "devDependencies": {
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "microbundle": "^0.11.0",
    "node-static": "^0.7.11",
    "prettier": "1.16.4",
    "semantic-release": "^15.9.9"
  },
  "dependencies": {
    "callforth": "^0.3.0",
    "inline-worker": "^1.1.0",
    "vue": "^2.6.10",
    "webrtc-adapter": "^6.2.1"
  },
  "repository": "github:gruhn/vue-qrcode-reader",
  "bugs": "https://github.com/gruhn/vue-qrcode-reader/issues",
  "homepage": "https://gruhn.github.io/vue-qrcode-reader",
  "license": "MIT",
  "keywords": [
    "vue",
    "vuejs",
    "vue-component",
    "qrcode",
    "qrcode-reader",
    "qrcode-scanner",
    "webrtc"
  ],
  "author": {
    "name": "Niklas Gruhn",
    "email": "niklas@gruhn.me"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js}": [
      "prettier --write"
    ]
  }
}
